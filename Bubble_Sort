#include<iostream>
using namespace std;

for(int i=0;i<n;i++){
  int ele = arr[i];
  for(int j = i;j<n;j++){
    if(arr[j]<arr[i]){
    flag = 1;
      swap(arr[i],arr[j])
    }
  }
  if(flag ==0){
    break;
  }
}

int main(){
  int arr[] = {1,4,3,6,5,3};
  bubble(arr,n);
  
}
